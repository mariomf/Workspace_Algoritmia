}

class Car {
	private String brand;
	private String type;
	private String color;
	
	Car(String brand, String type, String color){
		this.brand = brand;
		this.type  = type;
		this.color = color;
	};
	
	public String getBrand() {
		return brand;
	}
	public String getType() {
		return type;
	}
	public String getColor() {
		return color;
	}
}
